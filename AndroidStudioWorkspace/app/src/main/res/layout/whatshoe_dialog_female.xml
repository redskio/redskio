<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="337dp"
        android:layout_height="wrap_content"
        android:background="@drawable/roundlayout"
        android:layout_centerInParent="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/forwoman_title"
                android:id="@+id/forwoman_title"
                android:textColor="#000000"
                android:textSize="16sp"
                android:textStyle="bold"
                android:layout_alignParentTop="false"
                android:layout_centerInParent="true" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/dialog_cancel_btn"
                android:layout_alignParentEnd="true"
                android:src="@drawable/button_dialog_cancle"
                android:background="@null"
                android:layout_marginTop="7.25dp"
                android:layout_marginRight="15.5dp"
                android:layout_marginBottom="7.25dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="252dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="252dp"
                android:id="@+id/push_women_default"
                android:src="@drawable/push_women1"
                android:layout_gravity="center"
                android:layout_centerInParent="true" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="252dp"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:id="@+id/forwoman_wash_img"
                android:src="@drawable/push_women_shine"
                android:visibility="gone" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="252dp"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:id="@+id/forwoman_sub_img"
                android:src="@drawable/push_women_heel_small"
                android:visibility="gone" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="252dp"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:id="@+id/forwoman_submid_img"
                android:src="@drawable/push_women_heel_mid"
                android:visibility="gone" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="252dp"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:id="@+id/forwoman_subbig_img"
                android:src="@drawable/push_women_heel_big"
                android:visibility="gone" />

        </RelativeLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#eaeaea">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="56.5dp"
                    android:layout_weight="1"
                    android:id="@+id/forwoman_wash_layout"
                    android:background="#ffffff"
                    android:layout_marginTop="1.5dp"
                    android:layout_marginRight="0.75dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/forman_fix_wash_menu"
                        android:id="@+id/forwoman_wash_text"
                        android:textColor="#000000"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15.5dp"
                        android:textSize="12sp" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/forwoman_wash_check"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10.5dp"
                        android:button="@drawable/checkbox_dialog"
                        android:focusable="false"
                        android:clickable="false"/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="56.5dp"
                    android:layout_weight="1"
                    android:id="@+id/forwoman_sub_layout"
                    android:background="#ffffff"
                    android:layout_marginLeft="0.75dp"
                    android:layout_marginTop="1.5dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/forwoman_fix_sub_menu"
                        android:id="@+id/forwoman_sub_text"
                        android:textColor="#000000"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15.5dp"
                        android:textSize="12sp" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/forwoman_sub_check"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10.5dp"
                        android:button="@drawable/checkbox_dialog"
                        android:focusable="false"
                        android:clickable="false"/>
                </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#eaeaea">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="56.5dp"
                    android:layout_weight="1"
                    android:id="@+id/forwoman_submid_layout"
                    android:background="#ffffff"
                    android:layout_marginRight="0.75dp"
                    android:layout_marginTop="1.5dp"
                    android:layout_marginBottom="1.5dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/forwoman_fix_submid_menu"
                        android:id="@+id/forman_submid_text"
                        android:textColor="#000000"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15.5dp"
                        android:textSize="12sp" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/forwoman_submid_check"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10.5dp"
                        android:button="@drawable/checkbox_dialog"
                        android:focusable="false"
                        android:clickable="false"/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="56.5dp"
                    android:layout_weight="1"
                    android:id="@+id/forwoman_subbig_layout"
                    android:background="#ffffff"
                    android:layout_marginTop="1.5dp"
                    android:layout_marginBottom="1.5dp"
                    android:layout_marginLeft="0.75dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/forwoman_fix_subbig_menu"
                        android:id="@+id/forwoman_subbig_text"
                        android:textColor="#000000"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15.5dp"
                        android:textSize="12sp" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/forwoman_subbig_check"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10.5dp"
                        android:button="@drawable/checkbox_dialog"
                        android:focusable="false"
                        android:clickable="false"/>
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="71dp"
            android:minHeight="40dp"
            android:background="#eaeaea">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/dialog_totalprice"
                android:id="@+id/dialogTotalTitle"
                android:layout_alignParentTop="false"
                android:layout_alignParentStart="false"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15.5dp"
                android:textStyle="bold"
                android:textColor="#000000"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="15dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0ì›"
                android:id="@+id/dialogTotalPriceText"
                android:layout_alignParentTop="false"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.5dp"
                android:textStyle="bold"
                android:textColor="@color/color_login_default"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="15dp" />
        </RelativeLayout>

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="New Button"
            android:id="@+id/set_btn"
            android:background="@drawable/bottonroundlayout"
            android:layout_gravity="center_horizontal|bottom"
            android:textColor="#ffffff"
            android:textStyle="bold"
            android:layout_below="@+id/listView"
            android:layout_centerHorizontal="true"
            android:paddingTop="20dp"
            android:paddingBottom="20dp" />
    </LinearLayout>

</RelativeLayout>